{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2",
      "ClientCertificateMode": "AllowCertificate",
      "SslProtocols": [
        "Tls12",
        "Tls13"
      ]
    }
  }
}